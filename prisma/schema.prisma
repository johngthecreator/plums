generator client {
  provider = "prisma-client-js"
}


datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "foreignKeys"
}

model Space {
  space_id Int  @id @default(autoincrement())
  name String
  userId String
  notes Note[]
}

model Note {
  note_id Int  @id @default(autoincrement())
  title String
  data String
  space Space @relation(fields: [note_id], references: [space_id])
}
